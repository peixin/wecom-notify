{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./dist",
    "module": "COMMONJS",
    "target": "ESNext",
    "lib": [
      "ESNext"
    ],
    "alwaysStrict": true,
    "strict": true,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "esModuleInterop": true,
    "types": [
      "@cloudflare/workers-types"
    ],
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "test"
  ]
}